@model Medlebox.Models.Song

@Html.HiddenFor(m=>m.Gid)

<div class="control-group">
			<label class="control-label">Исполнитель</label>
			<div class="controls">@Html.TextBoxFor(model => model.Artist, new { @class = "span5" })  <span class="fve-wrapper">@Html.ValidationMessageFor(model => model.Artist)</span></div>
		</div>
<div class="control-group">
			<label class="control-label">Название</label>
			<div class="controls">@Html.TextBoxFor(model => model.Title, new { @class = "span5" })  @Html.ValidationMessageFor(model => model.Title)</div>
		</div>


<script type="text/javascript">
$('#Artist').typeahead({                                
  name: '',                                                          
  remote: '/api/songsAPI/getallartists?q=%QUERY',
  limit: 10
});

</script>